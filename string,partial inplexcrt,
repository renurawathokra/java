public class Main
{
public static void main(String[] args) {
 String s1 = "AJMER";
 for (int i = s1.length(); i >= 0; i--) 
{
 for (int j = 0; j < i; j++) 
 {
 System.out.print(" ");
 }
for (int k = i; k < s1.length(); k++)
{
System.out.print(s1.charAt(k));
}
 System.out.println();
}
}
}
