class Main {
     public static void main (String hhh[])
     
     {
         String s1="Renu";
         String s2="shalu";
         
         
         System.out.println(s1+s2);
         System.out.println(s1.concat(s2));
         System.out.println(s2.concat(s1));
     }
 }
 
