class Main{

public static void main (String []arr)
{
    String s1 = "Rajasthan";
    
    System.out.println(s1.toUpperCase());
     System.out.println(s1.toLowerCase());
    
     String s2 ="sonu ";
     String s3="      Devi       ";
     String s4=" Rawat";
     
     System.out.println(s2+s3+s4);
      System.out.println(s2+s3.trim()+s4);
      System.out.println(s2.trim()+s3.trim()+s4.trim());
     
    
}
}

